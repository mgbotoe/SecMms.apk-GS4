<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textColorHighlight">#ffff9200</item>
        <item name="android:textColorHint">?android:textColorHint</item>
        <item name="android:textColorLink">#ff5c5cff</item>
    </style>
    <style name="TextAppearance.SearchResult" parent="@style/TextAppearance">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimaryInverse</item>
        <item name="android:textColorHint">?android:textColorHintInverse</item>
    </style>
    <style name="TextAppearance.SearchResult.Title" parent="@style/TextAppearance.SearchResult">
        <item name="android:textSize">16.0sp</item>
    </style>
    <style name="TextAppearance.SearchResult.Subtitle" parent="@style/TextAppearance.SearchResult">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">?android:textColorSecondaryInverse</item>
    </style>
    <style name="MmsHoloTheme" parent="@*android:style/Theme.Holo.Light.DarkActionBar" />
    <style name="locationActionBar" parent="@style/Theme.DeviceDefault">
        <item name="android:windowActionBarOverlay">false</item>
    </style>
    <style name="templateStyle">
        <item name="android:textSize">22.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Theme.Translucent" parent="@style/Theme.DeviceDefault">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@*android:style/Animation</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="AcionBarMenuText">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@drawable/conversation_delete_actionbar_color</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/selector_actionbar_button_background</item>
        <item name="android:paddingLeft">16.5dip</item>
        <item name="android:paddingRight">16.5dip</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="WidgetTitle">
        <item name="android:textSize">19.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#fff0f0f0</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="NotificationPrimaryText">
        <item name="android:textColor">#ffcccccc</item>
    </style>
    <style name="NotificationSecondaryText">
        <item name="android:textColor">#ff666666</item>
    </style>
    <style name="NotificationSubjectText">
        <item name="android:textColor">#ff999999</item>
    </style>
    <style name="Theme.DeviceDefault" parent="@*android:style/Theme.DeviceDefault.Light" />
    <style name="Theme.QuickContact" parent="@*android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="WindowNoTitle">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="AppBaseTheme" parent="@*android:style/Theme.Light" />
    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="TwHelpDialogTheme" parent="@*android:style/Theme.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
    </style>
    <style name="NoAnimationDialog" parent="@*android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@null</item>
    </style>
    <style name="NoAnimation" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>
    <style name="TabTheme" parent="@style/Theme.DeviceDefault">
        <item name="android:actionBarStyle">@style/DialtactsActionBarStyle</item>
        <item name="android:actionBarSize">65.0dip</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle</item>
        <item name="android:actionBarTabTextStyle">@style/ActionBarTabTextStyle</item>
        <item name="android:actionBarNonEmbeded">true</item>
    </style>
    <style name="DialtactsActionBarStyle" parent="@*android:style/Widget.DeviceDefault.ActionBar">
        <item name="android:displayOptions">none</item>
    </style>
    <style name="ActionBarTabStyle" parent="@*android:style/Widget.DeviceDefault.ActionBar.TabView">
        <item name="android:padding">0.0dip</item>
        <item name="android:layout_margin">0.0dip</item>
        <item name="android:maxWidth">88.0dip</item>
        <item name="android:minWidth">80.0dip</item>
    </style>
    <style name="ActionBarTabTextStyle" parent="@*android:style/Widget.DeviceDefault.ActionBar.TabText">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TwMainListNoMessagesTextAppearance">
        <item name="android:textStyle">normal</item>
    </style>
</resources>
